{
  "name": "my_meals-backend",
  "version": "1.0.0",
  "description": "My_Meals project",
  "author": "Tyler Hayden",
  "license": "UNLICENSED",
  "repository": "https://github.com/tkHayden/My_Meals",
  "devDependencies": {
    "eslint": "*",
    "eslint-config-google": "*",
    "jest": "*",
    "nodemailer-mock": "^1.5.11",
    "nodemon": "*",
    "supertest": "*"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express-openapi-validator": "^4.13.7",
    "googleapis": "^100.0.0",
    "handlebars": "^4.7.7",
    "js-yaml": "^4.1.0",
    "nodemailer": "^6.7.3",
    "pg": "^8.7.3",
    "swagger-ui-express": "^4.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/server.js"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  },
  "eslintConfig": {
    "extends": [
      "google"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "indent": [
        "warn",
        2
      ]
    }
  },
  "scripts": {
    "start": "nodemon src/server.js",
    "test": "jest --runInBand --coverage --verbose --forceExit",
    "lint": "./node_modules/.bin/eslint . --ext .js"
  }
}
